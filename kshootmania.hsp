; mainloop
repeat
	switch scene
	case SCENE_TITLE
		start_title
		swbreak
	case SCENE_SELECT
		start_select
		swbreak
	case SCENE_BEFORE_PLAY
		courselist=""
		courselistnum=0
		coursenow=0
		coursetotalscore=0
		coursecritical=0
		coursenear=0
		courseerrorn=0
		courseend=0
		ccoursetitle=""
		f=0
		if(iscourse=1){
			buf=""
			chdir dir_default+"\\songs\\"+dir
			ccoursefile=cnotesfile
			notesel buf
			noteload cnotesfile
			if (lpeek(buf, 0) & 0x00FFFFFF) == 0xBFBBEF{
				is_course_utf8=1
				DeleteUTF8BOM buf
			}
			repeat notemax
				noteget stat1,cnt
				if(headcmp(stat1,6,"title=")){
					ccoursetitle=strmid(stat1,6,128)
					if(is_course_utf8=1):ccoursetitle=bom+strmid(stat1,6,128)
				}else:if(stat1="--"){
					f=1
				}else:if((f=1)&(strmid(stat1,0,1)="[")&(strmid(stat1,-1,1)="]")){
					stat1=strmid(stat1,1,strlen(stat1)-2)
					if(instr(stat1,0,"./")+instr(stat1,0,".\\")>-2){
						f=4
						break
					}
					course_sjis_ stat1
					exist dir_default+"\\songs\\"+stat1
					if(strsize<=0){
						exist dir_default+"\\songs\\"+minfostr.j.0.0+"\\"+strmid(stat1,instr(stat1,0,"/")+1,strlen(stat1)-instr(stat1,0,"/"))
						if(strsize<=0){
							f=4
							break
						}
					}
					courselist+=stat1+"\n"
					courselistnum++
				}else:if((stat1!="")&(f=1)){
					f=2
					break
				}
			loop
		}
		if((f=1)|(iscourse=0)){
			dim course_sc,courselistnum
			dim course_md,courselistnum
			dim course_gr,courselistnum
			repeat courselistnum
				course_gr.cnt=-1
			loop
			scene=SCENE_PLAY
		}else:if(f=2){
			dialog lang.2.12/*"このコースをプレイするには、最新バージョンのK-Shoot MANIAが必要です。"*/
			refreshsonglistoff=1
			scene=SCENE_SELECT
		}else:if(f=0){
			dialog lang.2.13/*"指定された譜面ファイルが見つかりません。"*/
			refreshsonglistoff=1
			scene=SCENE_SELECT
		}else{
			dialog lang.2.11/*"指定された譜面ファイルが見つかりません。"*/
			refreshsonglistoff=1
			scene=SCENE_SELECT
		}
		swbreak
	case SCENE_PLAY
		if((courseend!=1)&(cmdline_from=0)){
			whiteout 250
		}
		BASS_SetConfig 4,10000
		BASS_SetConfig 5,10000
		volzerofl=0
		plfl=0
		keshuoff=0
		if(iscourse=1){
			csongdir=""
			cnotesfile=".ksh"
			notesel courselist
			noteget stat2,coursenow
			sdim sndirstat,1,1
			split stat2,"/",sndirstat
			if(length(sndirstat)>=2){
				if(instr(stat2,0,"./")+instr(stat2,0,".\\")>-2){
					dialog lang.2.11/*"指定された譜面ファイルが見つかりません。"*/
					chdir dir_default+"\\songs"
					scene=SCENE_SELECT
					continue
				}
				exist dir_default+"\\songs\\"+stat2
				if(strsize>0){
					if(length(sndirstat)>=4){
						csongdir=sndirstat.0+"\\"+sndirstat.1+"\\"+sndirstat.2
					}else{
						csongdir=sndirstat.0+"\\"+sndirstat.1
					}
					cnotesfile=getpath(stat2,8)
				}else:if(iscmdline=1){
					dialog lang.2.13/*"指定された譜面ファイルが見つかりません。"*/
					end
				}else{
					exist dir_default+"\\songs\\"+minfostr.j.0.0+"\\"+strmid(stat2,instr(stat2,0,"/")+1,strlen(stat2)-instr(stat2,0,"/"))
					if(strsize>0){
						if(length(sndirstat)>=4){
							csongdir=dir+"\\"+sndirstat.1+"\\"+sndirstat.2
						}else{
							csongdir=dir+"\\"+sndirstat.1
						}
						cnotesfile=getpath(stat2,8)
					}
				}
			}
		}
		gsel 0
		redraw 0
		start_play
		swbreak
	case SCENE_RESULT
		on_update_result
		swbreak
	case SCENE_COURSE_RESULT
		on_update_course_result
		swbreak
	case SCENE_OPTION
		on_update_option
		swbreak
	case SCENE_GATE
		on_update_gate
		swbreak
	case SCENE_IR_SIGN_IN
		gsel 0
		selnow=standbyselp
		ddim curpos,1
		curpos=double(standbyselp)
		onkey gosub *keshu
		kp1=1 : kp2=1
		time_scene_start=d3timer()
		repeat
			redraw 0
			gosub *getjoystat
			timenow=d3timer()-time_scene_start
	
			// 背景表示
			gsel 0
			redraw 0
			gmode 0
			pos 0,0
			gcopy 29,0,0,scrsize_w,scrsize_h
	
			alCopyImageToScreen IMG_IR_SIGNIN, 0, scrsize_w / 2 - getsize(480) / 2, scrsize_h / 2 - getsize(320) / 2
			alCopyModeAlpha 0.3 + 0.7*(sin(double(d3timer())/150)+1.0)/2
			alCopyImageToScreen IMG_IR_SIGNIN_CURSOR, 0, scrsize_w / 2 - getsize(480) / 2 + getsize(64), scrsize_h / 2 - getsize(320) / 2 + getsize(205) + getsize(46) * ir_selnow
			alResetCopyMode
			
			if((timenow<250)&(timenow>=0)){
				gsel 0
				gmode 3,,,(250-timenow)*256/250
				color 0,0,0
				gsquare -1,sx_a,sy_a
			}
			
			if(ginfo2()=0){
				esc_key=getesckey()
				if((esc_key=1)&(escstatp=0)&(ginfo2()=0)){
					is_scene_initial_frame=1
					prev_scene=SCENE_IR_SIGN_IN
					scene=SCENE_TITLE
					standbyselp=0
					//BASS_ChannelSlideAttribute titlebgmid,2,0.0,100
					blackout 500
					redraw 0
					break
				}else:escstatp=esc_key
				getkey stat1,13
				stat1=stat1|getjoykeystat(11)
				if((stat1=1)&(esc_key=0)&(ginfo2()=0)){
					BASS_ChannelSetAttribute titlebgmid,2,double(mastervol)/100
					BASS_ChannelSlideAttribute titlebgmid,2,0.0,100
					stat1=BASS_SampleGetChannel(titleenterid,0)
					BASS_ChannelSetAttribute stat1,2,double(mastervol)/100
					BASS_ChannelPlay stat1
					if(ir_selnow=0){
						ir_login
					}else{
						session_id=""
						ksm_id=0
					}
					gsel 0,1
					blackout 500
					redraw 0
					is_scene_initial_frame=1
					refreshsonglistoff=0
					scene=SCENE_SELECT
					BASS_StreamFree titlebgmid
					break
				}
				getkey_a stat1,40
				getkey stat2,key.(7+2*analogsw)
				getkey stat2_,key2.(7+2*analogsw)
				stat1=stat1|stat2|stat2_
				getkey stat2,key.(9-2*analogsw)
				getkey stat2_,key2.(9-2*analogsw)
				stat1=stat1|stat2|stat2_|getjoykeystat(7+2*analogsw)|getjoykeystat(9-2*analogsw)
				getkey stat2,key.1
				getkey stat2_,key.1
				stat1=stat1|stat2|stat2_|getjoykeystat(1)
				if((kp1=0)&(stat1=1)&(ginfo2()=0)){
					ir_selnow=1
					stat1=BASS_SampleGetChannel(titleselid,0)
					BASS_ChannelSetAttribute stat1,2,double(mastervol)/100
					BASS_ChannelPlay stat1
					kp1=1
				}else:if((stat1=0)|(ginfo2()!=0)):kp1=0
				getkey_a stat1,38
				getkey stat2,key.(6+2*analogsw)
				getkey stat2_,key2.(6+2*analogsw)
				stat1=stat1|stat2|stat2_|getjoykeystat(6+2*analogsw)
				getkey stat2,key.(8-2*analogsw)
				getkey stat2_,key2.(8-2*analogsw)
				stat1=stat1|stat2|stat2_|getjoykeystat(8-2*analogsw)
				getkey stat2,key.2
				getkey stat2_,key.2
				stat1=stat1|stat2|stat2_|getjoykeystat(2)
				if((kp2=0)&(stat1=1)&(ginfo2()=0)){
					ir_selnow=0
					stat1=BASS_SampleGetChannel(titleselid,0)
					BASS_ChannelSetAttribute stat1,2,double(mastervol)/100
					BASS_ChannelPlay stat1
					kp2=1
				}else:if((stat1=0)|(ginfo2()!=0)):kp2=0
			}
			
			stat1=0
			getkey stat2,key.4
			if(stat2=1):stat1=1
			getkey stat2,key2.4
			if(stat2=1):stat1=1
			if(getjoykeystat(4)=1):stat1=1
			getkey stat2,key.5
			if(stat2=1):stat1=2
			getkey stat2,key2.5
			if(stat2=1):stat1=2
			if(getjoykeystat(5)=1):stat1=2
			redraw 1
			keshuoff=0
			await 7
			gosub *volactive
			if(timenow>600){
				while (ginfo2()!=0)
					await 100
				wend
			}
			timenowp=timenow
		loop
		swbreak
	swend
	if(cnt\(frameskip+1)=0){
		confkey=-1
		gsel 0
		redraw 1
		await (scene!=SCENE_GATE)*7
		gosub *volactive
		if((scene!=SCENE_GATE)&((iscourse!=1)&(autodemo=0))){
			while (ginfo2()!=0)
				await 100
			wend
		}
		if((analoginput=1)&(scene!=SCENE_OPTION)){
			keshucnt=0:onkey 1
		}
	}
	gsel 0
	redraw 0
loop
end

