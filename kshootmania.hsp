#deffunc blackout int blength
	buffer 2,scrsize_w,scrsize_h
	pos 0,0
	gmode 0
	gcopy 0,0,0,scrsize_w,scrsize_h
	bstartt=d3timer()
	gsel 0
	pos 0,0
	gmode 0
	repeat
		if(abs(d3timer()-bstartt)>blength){
			redraw 0
			gmode 0
			color 0,0,0
			boxf
			redraw 1
			break
		}
		redraw 0
		gcopy 2,0,0,scrsize_w,scrsize_h
		gmode 3,,,(d3timer()-bstartt)*256/blength
		color 0,0,0
		gsquare -1,sx_a,sy_a
		gmode 0
		redraw 1
		await 7
	loop
	return

