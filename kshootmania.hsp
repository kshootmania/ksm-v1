#deffunc settex2 int texid

	;	texture reg
	;
	workscrid=ginfo(3)
	winx=ginfo(12):winy=ginfo(13)

	px=1:repeat 12
	if px>=winx : break
	px=px<<1:loop
	py=1:repeat 12
	if py>=winy : break
	py=py<<1:loop
	if ( px!=winx )|( py!=winy ) {	; 2^nサイズでない時は補正
		buffer 3,px,py,0
		cls 4
		gmode 0
		pos 0,0
		gcopy workscrid,0,0,winx,winy
	}
	settex winx,winy,0,texid
	if stat<0 : dialog "Texture Error("+stat+")" { gsel hgsel : return }
	i=stat
	await 0
	gsel hgsel@HGIMG
	return i

