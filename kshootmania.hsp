		gcopy 2,0,0,scrsize_w,scrsize_h
		gmode 3,,,(d3timer()-wstartt)*256/wlength
		color 255,255,255
		gsquare -1,sx_a,sy_a
		gmode 0
		redraw 1
		await 7
	loop
	return

