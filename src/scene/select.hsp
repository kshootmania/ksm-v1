										if((levcnt.(19-cnt)<selnow)&(levcnt.(19-cnt)!=-1)){
											selnow=levcnt.(19-cnt)
											break
										}
									loop
									selnow=min(selnow,mnum-1)
								}
							}else{
								f=1
								repeat 20
									if(levcnt.cnt>selnow){
										selnow=levcnt.cnt
										f=0
										break
									}
								loop
								if(f=1):selnow=0
