				getkey stat2,key2.(4+cnt)
				if(stat2=1):stat1=1
				if(getjoykeystat(4+cnt)=1):stat1=1
				fxkey.cnt=stat1
			loop
			getkey stat1,37
			if((dialognow=0)&(timenow-playerminustp>160)&(btkey.1+btkey.2=2)&(stat1=1)&(ginfo2()=0)){
				cplayer--
				if(cplayer<0):cplayer=playersnum-1
